<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIDefaultSearchForm>
    <label>
      <keyword>Palavra chave</keyword>
      <portal>Portal</portal>
      <Page>Page</Page>
      <Document>Document</Document>
    </label>
    <action>
      <Search>Buscar</Search>
    </action>
  </UIDefaultSearchForm>

  <UIDefaultSearchResult>
    <label>
      <Page>Página</Page>
      <Document>Document</Document>
      <DocumentAndPage>Document and Page</DocumentAndPage>
      <documentandpage>Document and Page</documentandpage>
      <results>{0} resultados de {1} para {2}</results>
      <oneResult>Somente um resultado para {0}</oneResult>
      <ofAboutCaption>de cerca de</ofAboutCaption>
      <timeUnitCaption>seconds</timeUnitCaption>
      <similar>Páginas onde o mesmo conteúdo aparece</similar>
      <editor>Editor</editor>
      <date>Data</date>
      <relevancy>Relevância</relevancy>
      <title>Título</title>
      <sortBy>Sort By:</sortBy>
    </label>
    <action>
      <edit>Edit</edit>
    </action>
    <msg>
      <search-not-match-type>Sua pesquisa {0} não coincide com {1}.</search-not-match-type>
    </msg>
  </UIDefaultSearchResult>
  <UISearchPageLayoutManager>
    <label>
      <itemsPerPageSelector>Itens por página</itemsPerPageSelector>
      <searchFormSelector>Modelo de formulário de busca</searchFormSelector>
      <searchResultSelector>Modelo de resultado de busca</searchResultSelector>
      <searchPaginatorSelector>Modelo de paginador de busca</searchPaginatorSelector>
      <searchPageLayoutSelector>Modelo de página de busca</searchPageLayoutSelector>
      <searchResultBasePathInputSetAction>Caminho de base</searchResultBasePathInputSetAction>
      <pageMode>Modo página</pageMode>
      <option>
        <none>Nenhum</none>
        <more>More</more>
        <pagination>Pagination</pagination>
      </option>
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </UISearchPageLayoutManager>
  <searchResultBasePathInputSetAction>
    <title>
      <SelectBasePath>Selecione o caminho de Base</SelectBasePath>
    </title>
  </searchResultBasePathInputSetAction>  
  <UISearchForm>  
   <message>
     <keyword-not-support>A palavra-chave está inválida. Alguns caracteres não foram reconhecidos.</keyword-not-support>
     <not-checked>Por favor, selecione um tipo de busca.</not-checked>
     <keyword-not-empty>Insira uma palavra chave.</keyword-not-empty>
   </message>
  </UISearchForm>
  <UIPaginatorDefault>
    <label>
      <next>Próximo</next>
      <previous>Anterior</previous>
      <more>More</more>
    </label>
  </UIPaginatorDefault>
  <UISearchPageLayoutDefault>
    <label>
      <quickEditTitle>Edição rápida</quickEditTitle>
    </label>
  </UISearchPageLayoutDefault>
  <searchOption>
    <label>
      <pageCheckBox>nas páginas</pageCheckBox>
      <documentCheckBox>nos conteúdos</documentCheckBox>
    </label>
  </searchOption>
  <ViewMode>
    <label>
      <option>
        <all>buscar todos os sites</all>
      </option>  
    </label>  
  </ViewMode>
</bundle>
